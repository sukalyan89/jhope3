{
    "version": 2,
    "rewrites": [{ "source": "/(.*)", "destination": "/api" }],
    "builds": [
      { "src": "api/socket.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/socket.io/(.*)", "dest": "/api/socket.js" },
      { "src": "/", "dest": "/public/index.html" }
    ]
  }
  